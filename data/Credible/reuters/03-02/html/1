        f = window.ord || Math.floor(Math.random() * 1E16);
    iuUrl = Reuters.inlinevideo.adIuUrl + Reuters.inlinevideo.dartZone;
    var d = getUrlParameter("rpc"),
        e = getUrlParameter("RPC");
    if (d || e)
        iuUrl = iuUrl + "RPC" + e + d;
    d = "videoChannel%3D" + Reuters.inlinevideo.channelIds.replace(/,/g,
        "%2C") + "%26videoId%3D" + Reuters.inlinevideo.storyUrl.split('videoId=')[1] + "%26type%3D" + 'article_preroll' + "%26adfriendly%3D" + Reuters.inlinevideo.adfriendly;
    e = getUrlParameter("adstest");
    e && (d = d + "%26adstest%3D" + e);
    pageURL = document.querySelector("meta[property='og:url']").getAttribute("content");
    pageURL = encodeURIComponent(pageURL);
