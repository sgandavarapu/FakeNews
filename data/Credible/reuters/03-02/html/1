    var article_channels = Reuters.inlinevideo.channelIds.split(',');
    var suppressed_channels = [''];
    var suppress_RTV = false;

    for (var i=0; i < suppressed_channels.length; i++){
        if (article_channels.indexOf(suppressed_channels[i]) > -1) {
