                                        // See notes in App.js for explanation of optimizely experiment hooks
                                        (function() {

                                            function pollForOptimizelyExperiments(cb) {
                                                if (window.OptimizelyExperiments) {
                                                    return cb();
                                                }

                                                setTimeout(pollForOptimizelyExperiments, 50);
                                            }

                                            window.activateExperiment = function(experimentName) {
                                                pollForOptimizelyExperiments(function() {
                                                    window.OptimizelyExperiments.activate(experimentName);
                                                })
                                            }

                                            window.deactivateExperiment = function(experimentName) {
                                                pollForOptimizelyExperiments(function() {
                                                    window.OptimizelyExperiments.deactivate(experimentName);
                                                })
                                            }

                                        })();
