<script type="text/javascript">
    //Monitor scroll event and as soon as the user scrolls trigger the bottom ad to load
    jQuery(window).bind('scroll.loadbottomads', function() {
        //Trigger the ad
        googletag.pubads().refresh([DFPAdSlot_story_728x90_bottom]);
        //Unbind the scroll event so we don't trigger the ad again
        jQuery(window).unbind('.loadbottomads');
    });
</script>


<script type="text/javascript">
    //Monitor the mouse leave event and if it exits to the top then trigger the exit popup
    jQuery(window).on('mouseleave.exitpop', function(e) {
        if (e.clientY < 10 ) {
            ExitPop('C');
        }
    });
    //Get the markup for the exit popup and store it in the page to be triggered when the mouse goes out of the window at the top
    jQuery(document).ready(function(){ AnalyticsCustomEvent('ExitPop', 'Serve-C'); GetExitPop(238967,''); });
</script>


<!--script type="text/javascript">
    function GetGridLeftRail() {
        var url = "/controlpage.aspx?control=/common/controls/storygridonecol";
        AjaxCall(url, GetGridLeftRailSuccess, GetGridLeftRailError);
    }
    function RecalcLeftPositionLeftRailContent() {
        var l = jQuery('#divLeftRail').offset().left;
        jQuery('#divLeftRailContent').css('left',l+'px');
        //Recalcuate the height of the div that holds the grid squares
        var nav = jQuery('#divEntireNav').height();
        var header = jQuery('#headerAdSection').height();
        var pad = 40;
        var h = jQuery(window).height() - (nav + header + pad);
        jQuery('#DivStoryGridOneCol').css('height',h+'px');
    }
    function RecalcTopPositionLeftRailContent() {
        var nav = jQuery('#divEntireNav').height();
        var header = jQuery('#headerAdSection').height();
        var pad = 15;
        var content = jQuery('#DivStoryGridOneCol').height();
        var footer = jQuery('#ctl00_Footer1_divNoContent').offset().top;
        var scroll = jQuery(window).scrollTop();
        var top = 160;
        //Don't let the left rail content go higher than the nav bar
        top = (nav + header + pad) - scroll;
        if (top < 50) {
            top = 50;
        }
        //Don't let the left rail content go over the footer
        if (scroll > footer - (nav + pad + content)) {
            top = ((footer - (pad + content)) - scroll);
        }
        jQuery('#divLeftRailContent').css('top',top+'px');
    }
    function GetGridLeftRailSuccess(transport) {
        jQuery('#divLeftRailContent').html(transport);
        RecalcLeftPositionLeftRailContent();
        //Bind the scroll event on the left rail and when we get near the bottom call for more stories
        jQuery('#DivStoryGridOneCol').bind('scroll', function() {
            if (jQuery('#DivStoryGridOneCol').scrollTop() > jQuery('#storyGrid').height() - jQuery('#DivStoryGridOneCol').height() - 800) {
                if (!storyLoadInProcess) { 
                    StoryGetRows(); 
                }
            }
        });
        //Make sure that scrolling over the left content rail will not scroll the main page
        jQuery('#DivStoryGridOneCol').on('mousewheel DOMMouseScroll', function (e) {
            var e0 = e.originalEvent,
                delta = e0.wheelDelta || -e0.detail;
            this.scrollTop += ( delta < 0 ? 1 : -1 ) * 40;
            e.preventDefault();
        });
    }
    function GetGridLeftRailError(transport) {
        //Remove the left rail
        jQuery('#divStoryPageContainer').css('width','1006px');
        jQuery('#divStoryPageContainer').css('margin-left','0');
        jQuery('#divLeftRail').remove();
    }
    //Get the markup for the left rail grid control
    jQuery(document).ready(function(){  
        GetGridLeftRail();
    });

    jQuery(window).resize(function() {
        RecalcLeftPositionLeftRailContent();
    });
    jQuery(window).scroll(function() {
        RecalcTopPositionLeftRailContent();
    });
</script-->

