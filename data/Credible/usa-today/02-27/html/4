<script type="text/javascript">
    var StoryInSection2_activePage = 0;
    jQuery(document).ready(function () {
        jQuery(".jcarousel-tempButton").css("display", "none");
        jQuery('#StoryInSection2')
        jQuery('#StoryInSection2').jcarousel({ 
            scroll: 4, 
            animation: "fast",
            itemFirstInCallback: {onBeforeAnimation: StoryInSection2_updateDots}
        });
    });
    function StoryInSection2_updateDots(carouselInstance,liObject,positionIndex,action){
        if(action != "init"){
            if (positionIndex == 1){
                StoryInSection2_activePage = 0;
            }else{
                StoryInSection2_activePage = Math.ceil((positionIndex -1)/4);
            }
            AnalyticsCustomEvent("StoryInSectionCarouselLatest",action,"13-More Crime Stories");
            StoryInSection2_setBullets();
        }
    }
    function StoryInSection2_ScrollTo(item,animate,page){
            jQuery('#StoryInSection2').jcarousel("scroll",item,animate);
    }
    function StoryInSection2_setBullets(){
        jQuery(".bullets_StoryInSection2").removeClass("active").addClass("inactive");
        jQuery(".bullets_StoryInSection2.bullet_" + StoryInSection2_activePage).removeClass("inactive").addClass("active");
    }
</script>
