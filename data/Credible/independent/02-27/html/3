  if(typeof ga !== 'undefined'){
    ga('send', 'event', 'AdblockDisabled', 'Disabled', location.href);
  }
  $('.adblock-modal [data-action="close"]').click(function(e){ 
    $('.adblock-modal').remove(); 
  });
};
  });
}) (jQuery);


