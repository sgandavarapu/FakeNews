			s.linkTrackVars = 'prop38';
			s.prop38 = sharebarTracker;
			s.tl(this, 'o', sharebarTracker);
			s.prop38 = '';
		});
		jQuery('#gigya-share-btns-2 .gig-share-button tr img').bind('click', function(e) {
			var s = s_gi(s_account)
			sharebarTracker = 'Below Article Share';
